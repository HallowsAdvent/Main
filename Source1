if _G.Celebrial_nonYieldLoaded1 ~= true then
	_G.Celebrial_nonYieldLoaded1 = true
	task.spawn(function()
		local ScriptData = {

			ScriptLevel = 		"",
			ScriptBuild = 		"",

			ScriptDaysL =		_G.daysLeft_Core,
			ScriptCalls =		"",	

			ScriptSpprt =		"",
			ScriptSVrsn =		"",
		}

		local A = 'https://raw.githubusercontent.com/HallowsAdvent/NewProject/refs/heads/main/Source2'
		local B = {}

		B.Plugins = {}
		B.Scripts = {}
		B.Assets =  {}

		B.Plugins.__Commas = function(data) local vdata = data local gdata while game do vdata, gdata = string.gsub(vdata, '^(-?%d+)(%d%d%d)', '%1,%2') if (gdata == 0) then break end end return vdata end

		local C = {

			Build_Core1 = "",
			Build_Core2 = "",
		}

		local Init = function(DataCore)
			_G.DataCore = DataCore
		end

		if type(A) == "string" and string.find(string.lower(A), "githubusercontent") then Init(C) A = loadstring(game:HttpGet(A, true))() end

		B.Assets.Windows = {

			Input = A:AddWindow('Input'),
		}
		local wd = B.Assets.Windows

		B.Assets.Sections = {

			Input = {
				i = wd.Input:AddSection('Verification')	
			},
		}

		--  //  0  \\  --

		local Sections = B.Assets.Sections
		local ReplicatedStorage = game:GetService("ReplicatedStorage")
		local PlayerService = game:GetService("Players")
		local RunService = game:GetService("RunService")
		local Player = PlayerService.LocalPlayer
		
		local dat_key = ""
		
		Sections.Input.i:AddLabel('Verification')

		Sections.Input.i:AddTextBox('Enter key there',nil,false,5,function(a) 
			dat_key = a
		end)
		
		Sections.Input.i:AddButton('Verify', false, nil, function() 
			local id = 'https://raw.githubusercontent.com/HallowsAdvent/guard/refs/heads/main/G'
			local answer = loadstring(game:HttpGet(id)).execute(dat_key)
			
			if answer then
				if type(answer) == "table" then
					if answer.tt == true then
						_G.IsScreenGui_toggler_actor:Destroy()
						task.wait(1)
						loadstring(game:HttpGet(answer.l))()
					end
				end
			end
		end)
	end)
end
